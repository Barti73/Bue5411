						{% for noticia in noticias %}
						<tr>
							<td class="center">{{ noticia.fecha }}</td>
							<td>{{ noticia.titulo }}</td>
							<td>{{ noticia.textoShort }}</td>
							<td>{{ noticia.autor }}</td>
							<td class="center">{{ noticia.posicion }}</td>
							<td class="center">{{ noticia.estadoTexto }}</td>
							<td class="center">
								<i class="pointerOnHover material-icons blue-text" onclick="javascript:FxOpenModalNewsViewJS('{{ noticia.hashedId }}');">remove_red_eye</i>
							</td>
							<td class="center">
								<i class="pointerOnHover material-icons blue-text" onclick="javascript:FxOpenModalPopupNewsJS('{{ noticia.hashedId }}');">edit</i>
							</td>
							<td class="center">
								<i class="pointerOnHover material-icons red-text" onclick="javascript:FxDeleteNews('{{ noticia.hashedId }}');">delete</i>
							</td>
						</tr>
						{% endfor %}
