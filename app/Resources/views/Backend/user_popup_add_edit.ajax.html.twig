		<div id="modalPopup" class="modal">
			<div class="modal-content">
				<h5><b>{{ popupData.titulo }}</b></h5>
				<div class="divider"></div>
				<div class="section">
				
					<div class="row">
						<div class="col s12">
							<div class="row">
								<div class="input-field col s12">
									<input id="txtUsername" type="text" data-length="100" value="{{ popupData.userData.username }}">
									<label for="txtUsername" class="{{ popupData.class }}">Usuario (*)</label>
								</div>
							</div>
							<div class="row">
								<div class="input-field col s12">
									<input id="txtNombre" type="text" data-length="100" value="{{ popupData.userData.nombre }}">
									<label for="txtNombre" class="{{ popupData.class }}">Nombre (*)</label>
								</div>
							</div>
							<div class="row">
								<div class="col s12">
									<label class="active">Perfil</label>
									<p>
										<input name="radioPerfil" type="radio" id="radioUsuario" {% if popupData.userData.perfil == 2 %} checked {% endif %} />
										<label for="radioUsuario">Usuario</label>
									</p>
									<p>
										<input name="radioPerfil" type="radio" id="radioAdministrador" {% if popupData.userData.perfil == 1 %} checked {% endif %} />
										<label for="radioAdministrador">Administrador</label>
									</p>							
								</div>	 
							</div>
							<div class="row">
								<div class="col s12">
									<label class="active">Estado</label>
									<div class="switch">
										<label>Inactivo<input id="chkEstado" name="chkEstado" type="checkbox" {% if popupData.userData.estado == 1 %} checked {% endif %}>
											<span class="lever"></span>Activo
										</label>
									</div>
								</div>	 
							</div>
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<a href="javascript:void(0);" class=" modal-action modal-close waves-effect waves-green btn-flat ble">Cancelar</a>
					<a href="javascript:void(0);" class=" modal-action modal-close waves-effect waves-green btn-flat" onclick="javascript:FxSaveUserJS();">Guardar</a>
				</div>								
			</div>
		</div>
		<input id="hiddenUserIdHashed" name="hiddenUserIdHashed" type="hidden" value="{{ popupData.userData.hashedId }}">

